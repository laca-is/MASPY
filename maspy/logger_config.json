{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "simple": {
            "format": "[%(levelname)s] %(class_name)s:%(my_name)s> %(message)s"
        },
        "simple_json": {
            "()": "maspy.logger.MyJSONFormatter",
            "fmt_keys":{
                "level": "levelname",
                "desc": "desc",
                "function": "funcName"
            }
        },
        "json": {
            "()": "maspy.logger.MyJSONFormatter",
            "fmt_keys":{
                "level": "levelname",
                "desc": "desc",
                "system_time": "system_time",
                "logger": "name",
                "function": "funcName",
                "line": "lineno",
                "thread_name": "threadName"
            }
        }
    },
    "handlers": {
        "queue_handler": {
            "class": "logging.handlers.QueueHandler",
            "handlers": [],
            "respect_handler_level": true
        }
    },
    "loggers": {
        "maspy": {
            "level": "DEBUG",
            "handlers": [
                "queue_handler"
            ]
        }
    }
}